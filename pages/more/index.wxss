/**list.wxss**/
.list {
	width: 100%;
	padding: 20rpx;
	box-sizing: border-box;
	display: flex;
	justify-content: flex-start;
	flex-wrap: wrap;
}
.item {
	width: 25%;
	text-align: center;
	margin-bottom: 20rpx;
}
.img {
	width: 80rpx;
	height: 80rpx;
}
.text {
	font-size: 32rpx;
	color: #085aa1;
}
.ads {
	position: fixed;
	bottom: 0;
}
