<view hidden="{{!isShow}}">
    <view class="ys-mask"></view>
    <view class="ys-dialog">
        <ad unit-id="adunit-7d4437fb7ab7ffbe"></ad>
        <view class="ys-dialog-title">{{title}}</view>
        <view class="ys-dialog-content">{{content}}</view>
        <view class="ys-dialog-bottom">
          <view class="ys-dialog-btn" bindtap='cancel'>
            <button class="ys-dialog-cancel-btn">
              {{cancelText}}
            </button>
          </view>
          <view class="ys-dialog-btn">
            <button class="ys-dialog-ok-btn" catchtap="_okEvent">
            {{okText}}
            </button> 
          </view>
        </view>
    </view>
</view>